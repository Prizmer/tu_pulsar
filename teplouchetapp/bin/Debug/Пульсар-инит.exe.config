<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--################# НАСТРОЙКА ПАРСЕРА ШАБЛОНА #################-->
    <!--Порядковый номер строки с данными (не с заголовком). Отсчет с 1-->
    <add key="firstRow" value="2"/>
    <!--Порядковый номер колонки с квартирами. Отсчет с 1-->
    <add key="flatColumn" value="4"/>
    <!--Порядковый номер колонки с серийниками. Отсчет с 1-->
    <add key="factoryColumn" value="7"/>

    <!--Режим установки первоначальных значений. Отсчет с 1-->
    <add key="colIPIndex" value="4"/>
    <add key="colPortIndex" value="5"/>
    <add key="colChannelIndex" value="15"/>
    <add key="colMeterNameIndex" value="16"/>
    <add key="colPredValue" value="17"/>

    <!--############## НАСТРОЙКА ПАРАМЕТРОВ СОЕДИНЕНИЯ ##############-->
    <add key="parity" value="0"/>
    <add key="databits" value="8"/>
    <add key="stopbits" value="1"/>
    <add key="baudrate" value="9600"/>
    <add key="dtr" value="false"/>


  </appSettings>

</configuration>